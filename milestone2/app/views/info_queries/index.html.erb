<h1>Listing info_queries</h1>

<table>
  <thead>
    <tr>
      <th>Is owner</th>
      <th>User size</th>
      <th>Oppo size</th>
      <th>User loc</th>
      <th>Oppo loc</th>
      <th>Law issue</th>
      <th>Tech area</th>
      <th>Judge1</th>
      <th>Judge2</th>
      <th>Judge3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @info_queries.each do |info_query| %>
      <tr>
        <td><%= info_query.is_owner %></td>
        <td><%= info_query.user_size %></td>
        <td><%= info_query.oppo_size %></td>
        <td><%= info_query.user_loc %></td>
        <td><%= info_query.oppo_loc %></td>
        <td><%= info_query.law_issue %></td>
        <td><%= info_query.tech_area %></td>
        <td><%= info_query.judge1 %></td>
        <td><%= info_query.judge2 %></td>
        <td><%= info_query.judge3 %></td>
        <td><%= link_to 'Show', info_query %></td>
        <td><%= link_to 'Edit', edit_info_query_path(info_query) %></td>
        <td><%= link_to 'Destroy', info_query, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info query', new_info_query_path %>
